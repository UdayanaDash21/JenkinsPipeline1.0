pipeline{
    agent any
    tools{
        maven "maven"
    }
    enviornment{
        VERSION_NAME="1.02"
    }

    stages{
        stage("Compile"){
           steps{
             sh 'javac Test.java'
             sh 'echo "${VERSION_NAME}"'
           }
        }
          stage("Run"){

            steps{
           sh 'java Test'
            }
        }
    }

    post{
        always{
            sh 'echo "Status of Build"'
        }
        success{
            sh 'echo "build success"'
        }
        failure{
            sh 'echo "build failed"'
        }
    }


}
///