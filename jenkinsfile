pipeline{
    agent any
    stages{
        stage("Compile"){
           steps{
             sh 'javac Test.java'
           }
        }
          stage("Run"){

            steps{
           sh 'java Test'
            }
        }
    }

    post{
        always{
            sh 'echo "Status of Build"'
        }
        success{
            sh 'echo "build success"'
        }
        failure{
            sh 'echo "build failed"'
        }
    }

    
}
///